issn = {1049-5258},
keywords = {Algorithms,Biological,Eye Movements,Eye Movements: physiology,Fixation,Humans,Models,Ocular,Ocular: physiology,Pattern Recognition,Reproducibility of Results,Saccades,Vision,Visual,Visual Perception,Visual Perception: physiology},
month = jan,
