issn = {1553-7358},
journal = {PLoS computational biology},
keywords = {Artificial Intelligence,Biological,Biomimetics,Biomimetics: methods,Computer Simulation,Computer-Assisted,Computer-Assisted: methods,Humans,Image Interpretation,Models,Pattern Recognition,Visual,Visual: physiology,printed},
mendeley-tags = {printed},
month = jan,
