issn = {1476-4687},
journal = {Nature},
keywords = {Brain,Brain: physiology,Computer Simulation,Eye Movements,Eye Movements: physiology,Fixation,Humans,Ocular,Ocular: physiology,Photic Stimulation,Psychomotor Performance,Psychomotor Performance: physiology,Vision,printed},
