doi = {10.1162/NECO_a_00086},
file = {:Users/sergeyk/Dropbox/Application_Data/Mendeley2/Le Roux et al./2010/Le Roux et al. - 2010 - Learning a Generative Model of Images by Factoring Appearance and Shape.pdf:pdf},
